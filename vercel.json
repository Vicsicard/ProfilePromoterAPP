{
  "version": 2,
  "builds": [
    {
      "src": "onboarding-app/package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "cd onboarding-app && npm install && npm run build",
        "installCommand": "cd onboarding-app && npm install",
